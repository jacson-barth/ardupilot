		<engine-rumble>
			<name>engine-rumble</name>
			<path>Sounds/rumble.wav</path>
			<mode>looped</mode>
			<condition>
				<not>
					<property>engines/engine[0]/out-of-fuel</property>
				</not>
<or>
    			<property>engines/engine[0]/running</property>
    			<property>engines/engine[1]/running</property>
</or>
			</condition>
			<orientation>
				<x>0.0</x>
				<y>0.0</y>
				<z>-180.0</z>
				<inner-angle>180.0</inner-angle>
				<outer-angle>360.0</outer-angle>
				<outer-gain>0.2</outer-gain>
			</orientation>
			<pitch>
				<property>engines/engine[0]/augmentation</property>
				<factor>1</factor>
				<offset>0.3</offset>
			</pitch>
			<volume>
				<property>engines/engine[0]/afterburner</property>
				<!--<factor>0.7</factor>-->
				<factor>1.2</factor>
				<offset>0.15</offset>
				<!--<max>1</max>-->
			</volume>
		</engine-rumble>
		<engine-whine-external>
			<name>external-whine</name>
			<path>Aircraft/f-14b/Sounds/jet-whine.wav</path>
			<mode>looped</mode>
			<condition>
				<not>
					<property>sim/current-view/internal</property>
				</not>
				<not>
					<property>engines/engine[0]/out-of-fuel</property>
				</not>
<or>
    			<property>engines/engine[0]/running</property>
    			<property>engines/engine[1]/running</property>
</or>
			</condition>
			<orientation>
				<x>0.0</x>
				<y>0.0</y>
				<z>1</z>
				<inner-angle>180.0</inner-angle>
				<outer-angle>360.0</outer-angle>
				<outer-gain>0.5</outer-gain>
			</orientation>
			<pitch>
				<property>engines/engine[0]/n1</property>
				<ofset>0.7</ofset>
				<factor>.003</factor>
			</pitch>
			<volume>
				<property>engines/engine/n1</property>
				<offset>1.0</offset>
				<factor>-0.008</factor>
				<max>0.6</max>
			</volume>
		</engine-whine-external>
		<engine-whine-internal>
			<name>internal-whine</name>
